# Amazon Crawler Configuration
PORT=8080
GIN_MODE=release

# Auth Configuration
JWT_SECRET=your_jwt_secret_here
API_KEY=your_api_key_here

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_DURATION=60

# Crawler Settings
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36
REQUEST_TIMEOUT=30s
RETRY_ATTEMPTS=3
RETRY_DELAY=2s

# Proxy Settings (optional)
PROXY_URL=
PROXY_USERNAME=
PROXY_PASSWORD=

# Debug Settings
DEBUG=false
LOG_level=info
